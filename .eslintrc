{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "comma-dangle": "off",
    "eol-last": "off",
    "import/no-named-as-default-member": "off",
    "import/no-named-as-default": "off",
    "react/prefer-stateless-function": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off"
  }
}